{
    "root": true,
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
      "parser": "babel-eslint",
      "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module",
        "allowImportExportEverywhere": true
      },
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "consistent-return":"off"
    }
  }